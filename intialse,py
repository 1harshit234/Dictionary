employees = ['Kelly', 'Emma']
defaults = {"designation": 'Developer', "salary": 8000}
for i in employees:
    
    y = {i:defaults}
    print(y,end = '')





    

##method 2
employees = ['Kelly', 'Emma']
defaults = {"designation": 'Developer', "salary": 8000}
res = dict.fromkeys(employees, defaults)
print(res)

